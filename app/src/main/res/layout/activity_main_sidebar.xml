<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="313dp"
        android:layout_height="673dp"
        android:layout_marginEnd="103dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sidebar" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="@dimen/imageView11"
        android:layout_height="64dp"
        android:layout_marginTop="-650dp"
        android:layout_marginEnd="-110dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@drawable/man" />

    <TextView
        android:id="@+id/greeting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="-170dp"
        android:fontFamily="@font/solway_bold"
        android:text="@string/greeting"
        android:textColor="@color/greeting"
        android:textSize="@dimen/greeting"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView11" />

    <TextView
        android:id="@+id/profile_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-450dp"
        android:layout_marginEnd="-178dp"
        android:drawableLeft="@drawable/baseline_person_24"
        android:drawablePadding="5dp"
        android:fontFamily="@font/solway_bold"
        android:text="@string/profile_txt"
        android:textColor="@color/profile_txt"
        android:textSize="@dimen/profile_txt"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <TextView
        android:id="@+id/notify_sidebar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="-192dp"
        android:drawableLeft="@drawable/baseline_notifications_24"
        android:drawablePadding="5dp"
        android:fontFamily="@font/solway_bold"
        android:text="@string/notify_sidebar"
        android:textColor="@color/notify_sidebar"
        android:textSize="@dimen/notify_sidebar"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/profile_txt" />

    <TextView
        android:id="@+id/bill_sidebar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="-217dp"
        android:drawableLeft="@drawable/baseline_add_circle_24"
        android:drawablePadding="5dp"
        android:fontFamily="@font/solway_bold"
        android:text="@string/bill_sidebar"
        android:textColor="@color/bill_sidebar"
        android:textSize="@dimen/bill_sidebar"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/notify_sidebar" />

    <TextView
        android:id="@+id/electricity_sidebar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-290dp"
        android:layout_marginEnd="-225dp"
        android:drawableLeft="@drawable/baseline_electric_bolt_24"
        android:fontFamily="@font/solway_bold"
        android:text="@string/electricity_sidebar"
        android:textColor="@color/electricity_sidebar"
        android:textSize="@dimen/electricity_sidebar"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <TextView
        android:id="@+id/rate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-130dp"
        android:layout_marginEnd="-135dp"
        android:drawableLeft="@drawable/baseline_star_24"
        android:fontFamily="@font/solway_bold"
        android:text="@string/rate"
        android:textColor="@color/rate"
        android:textSize="@dimen/rate"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <TextView
        android:id="@+id/water_sidebar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="-181dp"
        android:drawableLeft="@drawable/baseline_water_drop_24"
        android:fontFamily="@font/solway_bold"
        android:text="@string/water_sidebar"
        android:textColor="@color/water_sidebar"
        android:textSize="@dimen/water_sidebar"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/electricity_sidebar" />

    <TextView
        android:id="@+id/gas_sidebar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="-156dp"
        android:drawableLeft="@drawable/baseline_gas_meter_24"
        android:fontFamily="@font/solway_bold"
        android:text="@string/gas_sidebar"
        android:textColor="@color/gas_sidebar"
        android:textSize="@dimen/gas_sidebar"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/water_sidebar" />

    <TextView
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-130dp"
        android:layout_marginEnd="-270dp"
        android:fontFamily="@font/solway_bold"
        android:text="@string/logout"
        android:textColor="@color/logout"
        android:textSize="@dimen/logout"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <Switch
        android:id="@+id/dark_mode"
        android:layout_width="175dp"
        android:layout_height="48dp"
        android:layout_marginTop="-520dp"
        android:layout_marginEnd="-202dp"
        android:fontFamily="@font/solway_bold"
        android:text="@string/dark_mode"
        android:textColor="@color/dark_mode"
        android:textSize="@dimen/dark_mode"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

</androidx.constraintlayout.widget.ConstraintLayout>